{
  "expo": {
    "name": "Happy Baby Night Nurses",
    "slug": "happy-baby-night-nurses",
    "version": "1.0.0",
    "owner": "devteam2025",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "happybaby",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.happybaby.iphone",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#4F86E7"
      },
      "package": "com.happybaby.nightnurses",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your nurse.",
          "cameraPermission": "The app accesses your camera to let you take photos to share with your nurse."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "090e1d7a-381b-447c-bff2-da76583d77ca"
      }
    }
  }
}
